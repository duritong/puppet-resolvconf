# managed by puppet
<% if @domain -%>
domain <%= @domain  %>
<% end -%>
<% if @search -%>
search <%= @search %>
<% end -%>

<% Array(@nameservers).each do |nameserver|
  unless nameserver.nil? || nameserver.empty?-%>
nameserver <%= nameserver %>
<% end
end -%>

<% if @tail -%>
<%= tail %>
<% end -%>
